@model IEnumerable<AccesPoint.Models.User>
@{
}

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Email</th>
            <th>PhoneNumber</th>
            <th>EmailConfirmed</th>
            <th>ProviderDisplayName</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
                <td>@user.EmailConfirmed</td>
                <td>@user.ProviderDisplayName</td>
            </tr>
        }
    </tbody>
</table>